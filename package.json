{
	"name": "vue-versatile",
	"displayName": "vue-versatile",
	"description": "一款提高vue项目开发效率的插件",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.37.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.delete.console.log"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [{
            "command": "extension.delete.console.log",
			"title": "删除console.log"
		}],
		"menus": {
			"editor/context": [{
				"command": "extension.delete.console.log",
				"group": "9_cutcopypaste"
			}]
		}
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.37.0",
		"eslint": "^5.13.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"vscode-test": "^1.0.2"
	}
}
